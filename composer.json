{
  "name": "zynga/phpunit",
  "description": "Zynga modifications to the PHPUNIT framework.",
  "type": "library",
  "keywords": [
    "phpunit",
    "xunit",
    "testing"
  ],
  "bin": [
    "bin/phpunit",
    "bin/phpunit-xhprof",
    "bin/phpunit.hh",
    "bin/run-single-test",
    "bin/run-single-test-xhprof",
    "bin/run-single-test.hh",
    "bin/hhvm-restart-wrapper"
  ],
  "require": {
    "hhvm": "^3.18",
    "zynga/phpunit-hhi": "@dev"
  },
  "autoload": {
    "psr-4": {
      "PHPUnit\\Tests\\": "tests/phpunit/PHPUnit/Tests/",
      "SebastianBergmann\\CodeCoverage\\": "src/SebastianBergmann/CodeCoverage/",
      "SebastianBergmann\\TokenStream\\": "src/SebastianBergmann/TokenStream/",
      "SebastianBergmann\\TokenStream\\Tests": "tests/token-stream/SebastianBergmann/TokenStream/Tests/",
      "Zynga\\PHPUnit\\V2\\": "src/Zynga/PHPUnit/V2/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:Zynga/zynga-hhvm-phpunit-hhi.git"
    }
  ]
}
